<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" x:Class="Gamma.Dialogs.ChoosePrintNameDialog"
        Title="Имя для печати" Height="120" Width="571"
        WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Grid>

        <dxe:ComboBoxEdit x:Name="EdtPrintName" Margin="10,43,4,0" VerticalAlignment="Top" AutoComplete="True" FindMode="Always" EditValueChanged="EdtPrintName_EditValueChanged"/>
        <Label Content="Имя для печати" Margin="10,10,347,0" VerticalAlignment="Top" Height="28"/>
        <Button x:Name="btnOK" Content="OK" Margin="0,70,10,10" IsDefault="True" HorizontalAlignment="Right" Click="BtnOK_Click" Width="78" IsEnabled="False" />
    </Grid>
</Window>
